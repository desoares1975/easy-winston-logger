{
  "name": "easy-winston-logger",
  "version": "0.10.4",
  "description": "Logger for Node.js servers",
  "main": "index.js",
  "scripts": {
    "coverage": "npm run test | coveralls",
    "test": "jest --ci --collectCoverage",
    "testdev": "nodemon --exec jest"
  },
  "author": "desoares1975",
  "repository": {
    "type": "git",
    "url": "git@github.com:desoares1975/easy-winston-logger.git"
  },
  "license": "MIT",
  "dependencies": {
    "winston": "^3.6.0",
    "winston-daily-rotate-file": "^4.6.1",
    "winston-mongodb": "^5.0.7"
  },
  "homepage": "git@github.com:desoares1975/easy-winston-logger.gitr#readme",
  "devDependencies": {
    "jest": "^28.1.0",
    "nodemon": "^2.0.15"
  },
  "keywords": [
    "logger",
    "winston",
    "easy",
    "wrapper",
    "leveled",
    "logger"
  ],
  "jest": {
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  }
}
